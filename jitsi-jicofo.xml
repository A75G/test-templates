<?xml version="1.0"?>
<Container version="2">
  <Name>jitsi-jicofo</Name>
  <Repository>tiredofit/jitsi-jicofo</Repository>
  <Registry>https://hub.docker.com/r/tiredofit/jitsi-jicofo/</Registry>
  <Network>bridge</Network>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support>https://hub.docker.com/r/tiredofit/jitsi-jicofo/</Support>
  <Project/>
  <Overview>Dockerized Jitsi Conference Focus for Integration with Jitsi Meet WebRTC conferencing solution</Overview>
  <Category/>
  <WebUI/>
  <TemplateURL/>
  <Icon>/plugins/dynamix.docker.manager/images/question.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <DonateText/>
  <DonateLink/>
  <Description>Dockerized Jitsi Conference Focus for Integration with Jitsi Meet WebRTC conferencing solution</Description>
  <Config Name="Certs" Target="/certs" Default="/mnt/user/appdata/jitsi/certs/" Mode="rw" Description="Needed to share certificates between containers for Self Signed variants" Type="Path" Display="always" Required="false" Mask="false">/mnt/user/appdata/jitsi/certs/</Config>
  <Config Name="Logs" Target="/var/log/jitsi" Default="/mnt/user/appdata/jitsi/logs/jicofo/" Mode="rw" Description="Logs" Type="Path" Display="always" Required="false" Mask="false">/mnt/user/appdata/jitsi/logs/jicofo/</Config>
  <Config Name="HOST" Target="HOST" Default="meet.example.com" Mode="" Description="Hostname of your server e.g. meet.example.com Should be same as all other hostnames" Type="Variable" Display="always" Required="false" Mask="false">meet.example.com</Config>
  <Config Name="PROSODY_HOST" Target="PROSODY_HOST" Default="jitsi-prosody" Mode="" Description="Container Name of your prosody server e.g. prosody" Type="Variable" Display="always" Required="false" Mask="false">jitsi-prosody</Config>
  <Config Name="JICOFO_PASS" Target="JICOFO_PASS" Default="secret2" Mode="" Description="Jitsi Conference Focus Secret" Type="Variable" Display="always" Required="false" Mask="false">secret2</Config>
  <Config Name="JICOFO_USER_PASS" Target="JICOFO_USER_PASS" Default="secret1" Mode="" Description="Jitsi Conference Focus User Secret" Type="Variable" Display="always" Required="false" Mask="false">secret1</Config>
</Container>