<?xml version="1.0"?>
<Container version="2">
  <Name>jitsi-prosody</Name>
  <Repository>tiredofit/jitsi-prosody</Repository>
  <Registry>https://hub.docker.com/r/jitsi/prosody/</Registry>
  <Network>bridge</Network>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support>https://hub.docker.com/r/jitsi/prosody/</Support>
  <Project/>
  <Overview>Prosody image for Jitsi Meet</Overview>
  <Category/>
  <WebUI/>
  <TemplateURL/>
  <Icon>/plugins/dynamix.docker.manager/images/question.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <DonateText/>
  <DonateLink/>
  <Description>Prosody image for Jitsi Meet</Description>
  <Config Name="Certs" Target="/certs" Default="/mnt/user/appdata/jitsi/certs/" Mode="rw" Description="Needed to Automatically Generate Certificates for other containers" Type="Path" Display="always" Required="false" Mask="false">/mnt/user/appdata/jitsi/certs/</Config>
  <Config Name="Logs" Target="/var/log/prosody" Default="/mnt/user/appdata/jitsi/logs/prosody/" Mode="rw" Description="Logs" Type="Path" Display="always" Required="false" Mask="false">/mnt/user/appdata/jitsi/logs/prosody/</Config>
  <Config Name="HOST" Target="HOST" Default="meet.example.com" Mode="" Description="Hostname of your server e.g. meet.example.com Should be same as all other hostnames" Type="Variable" Display="always" Required="false" Mask="false">meet.example.com</Config>
  <Config Name="JITSI_VIDEO_PASS" Target="JITSI_VIDEO_PASS" Default="secret1" Mode="" Description="Container Variable: JITSI_VIDEO_PASS" Type="Variable" Display="always" Required="false" Mask="false">secret1</Config>
  <Config Name="JICOFO_PASS" Target="JICOFO_PASS" Default="secret2" Mode="" Description="Container Variable: JICOFO_PASS" Type="Variable" Display="always" Required="false" Mask="false">secret2</Config>
  <Config Name="JICOFO_USER_PASS" Target="JICOFO_USER_PASS" Default="secret3" Mode="" Description="Container Variable: JICOFO_USER_PASS" Type="Variable" Display="always" Required="false" Mask="false">secret3</Config>
  <Config Name="Prosody Client" Target="5222" Default="5222" Mode="tcp" Description="Prosody Client" Type="Port" Display="always" Required="false" Mask="false">5222</Config>
  <Config Name="Prosody Server" Target="5280" Default="5280" Mode="tcp" Description="Prosody Server" Type="Port" Display="always" Required="false" Mask="false">5280</Config>
  <Config Name="Prosody Components" Target="5347" Default="5347" Mode="tcp" Description="Prosody Components" Type="Port" Display="always" Required="false" Mask="false">5347</Config>
</Container>