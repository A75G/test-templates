<?xml version="1.0"?>
<Container version="2">
  <Name>jitsi-meet</Name>
  <Repository>jitsi/web</Repository>
  <Registry>https://hub.docker.com/r/jitsi/web/</Registry>
  <Network>bridge</Network>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support>https://hub.docker.com/r/jitsi/web/</Support>
  <Project/>
  <Overview>Jitsi Meet's web interface image</Overview>
  <Category/>
  <WebUI/>
  <TemplateURL/>
  <Icon>/plugins/dynamix.docker.manager/images/question.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <DonateText/>
  <DonateLink/>
  <Description>Jitsi Meet's web interface image</Description>
  <Config Name="Assets" Target="/assets/jitsi-meet" Default="/mnt/user/appdata/jitsi/assets/jitsi-meet/" Mode="rw" Description="Put your custom config.js/interfaceConfig.js in here and it will be added on bootup" Type="Path" Display="always" Required="false" Mask="false">/mnt/user/appdata/jitsi/assets/jitsi-meet/</Config>
  <Config Name="Logs" Target="/var/log/nginx" Default="/mnt/user/appdata/jitsi/logs/nginx/" Mode="" Description="Logs" Type="Variable" Display="always" Required="false" Mask="false">/mnt/user/appdata/jitsi/logs/nginx/</Config>
  <Config Name="Certs" Target="/certs" Default="/mnt/user/appdata/jitsi/certs/" Mode="" Description="Needed to share certificates between containers for Self Signed variants" Type="Variable" Display="always" Required="false" Mask="false">/mnt/user/appdata/jitsi/certs/</Config>
  <Config Name="PROSODY_HOST" Target="PROSODY_HOST" Default="jitsi-prosody" Mode="" Description="Container Name of your prosody server" Type="Variable" Display="always" Required="false" Mask="false">jitsi-prosody</Config>
  <Config Name="Port" Target="80" Default="8080" Mode="tcp" Description="Port" Type="Port" Display="always" Required="false" Mask="false">8080</Config>
  <Config Name="JITSI_VIDEO_PASS" Target="JITSI_VIDEO_PASS" Default="secret1" Mode="" Description="Container Variable: JITSI_VIDEO_PASS" Type="Variable" Display="always" Required="false" Mask="false">secret1</Config>
  <Config Name="JICOFO_PASS" Target="JICOFO_PASS" Default="secret2" Mode="" Description="Container Variable: JICOFO_PASS" Type="Variable" Display="always" Required="false" Mask="false">secret2</Config>
  <Config Name="JICOFO_USER_PASS" Target="JICOFO_USER_PASS" Default="secret3" Mode="" Description="Container Variable: JICOFO_USER_PASS" Type="Variable" Display="always" Required="false" Mask="false">secret3</Config>
</Container>